<% content_for :title do %>
	Display all users
<% end %>
<h1>Display All Users</h1>

<table class="table">
	<thead>
		<th>First name</th>
		<th>Last name</th>
		<th>Email address</th>
	</thead>
<% @users.each do |user| %>
	<tr>
		<td><%= user.first_name %></td>		
		<td><%= user.last_name %></td>
		<td><%= user.email_address %></td>
	</tr>
<% end %>
</table>